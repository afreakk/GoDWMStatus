{
    "minimum_render_interval": "150ms",
    "modules": [
        {
            "name": "memory",
            "cron": "@every 5s",
            "bar_filled": "▰",
            "bar_empty": "▱",
            "sprintf": "mem %s%s"
        },
        {
            "name": "cpu",
            "cron": "@every 5s",
            "bar_width": 10,
            "bar_filled": "▰",
            "bar_empty": "▱",
            "sprintf": "cpu %s%s"
        },
        {
            "name": "readfile",
            "cron": "@every 5s",
            "file_path": "/sys/class/thermal/thermal_zone2/temp",
            "sprintf": "%.2s℃"
        },
        {
            "name": "readfile",
            "cron": "@every 5s",
            "file_path": "/sys/class/drm/card0/device/hwmon/hwmon2/temp1_input",
            "sprintf": "gpu %.2s℃"
        },
        {
            "name": "readfile",
            "cron": "@every 5s",
            "file_path": "/sys/class/drm/card0/device/hwmon/hwmon2/pwm1",
            "sprintf": "fan_pwm %s"
        },
        {
            "name": "readfile",
            "cron": "@every 5s",
            "file_path": "/sys/class/drm/card0/device/hwmon/hwmon2/pwm1_enable",
            "sprintf": "fan_mode %s"
        },
        {
            "name": "datetime",
            "sprintf": "[%s]",
            "datetime_format": "15:04 2/Jan",
            "cron": "0 * * * * *"
        }
    ]
}
