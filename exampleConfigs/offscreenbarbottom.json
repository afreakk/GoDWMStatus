{
    "wmclient": "sway",
    "minimum_render_interval": "150ms",
    "modules": [{
        "name": "memory",
        "cron": "@every 5s",
        "instance": "lol",
        "color": "#00FFD5",
        "background": "#303030",
        "border": "#000000",
        "min_width": 200,
        "align": "center",
        "separator": true,
        "separator_block_width": 9,

        "bar_width": 15,
        "bar_filled": "=",
        "bar_empty": " ",
        "sprintf": "mem: [<span fgcolor='#FD00E1'>%s%s</span>]",
        "markup": "pango"
    },{
        "name": "cpu",
        "cron": "@every 5s",
        "instance": "cpu-uzg",
        "color": "#00FFD5",
        "background": "#303030",
        "border": "#000000",
        "min_width": 500,
        "align": "right",
        "separator": false,
        "separator_block_width": 1,

        "bar_width": 15,
        "bar_filled": "=",
        "bar_empty": " ",
        "sprintf": "cpu: [<span fgcolor='#FD00E1'>%s%s</span>]",
        "markup": "pango"
    },{
        "name": "readfile",
        "cron": "@every 5s",
        "file_path": "/sys/class/thermal/thermal_zone2/temp",
        "sprintf": "<span fgcolor='#FD00E1'>%.2s℃</span>",
        "instance": "xx",
        "color": "#00FFD5",
        "background": "#303030",
        "border": "#000000",
        "min_width": 110,
        "align": "left",
        "separator": false,
        "separator_block_width": 9,
        "markup": "pango"
    },{
        "name": "readfile",
        "cron": "@every 5s",
        "file_path": "/sys/class/drm/card0/device/hwmon/hwmon2/temp1_input",
        "sprintf": "[gpu <span fgcolor='#FD00E1'>%.2s℃</span>]",
        "instance": "yy",
        "color": "#00FFD5",
        "background": "#303030",
        "border": "#000000",
        "min_width": 110,
        "align": "center",
        "separator": false,
        "separator_block_width": 9,
        "markup": "pango"
    },{
        "name": "datetime",
        "datetime_format": "[<span fgcolor='#FD00E1'>15:04 2/Jan</span>]",
        "cron": "0 * * * * *",
        "instance": "1",
        "color": "#00FFD5",
        "background": "#303030",
        "border": "#000000",
        "min_width": 350,
        "align": "right",
        "separator": true,
        "separator_block_width": 9,
        "markup": "pango"
    }]
}
