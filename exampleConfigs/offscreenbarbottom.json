{
    "wmclient": "sway",
    "minimum_render_interval": "150ms",
    "modules": [{
        "name": "memory",
        "cron": "@every 5s",
        "instance": "lol",
        "color": "#00FFD5",
        "min_width": 200,
        "align": "center",

        "bar_width": 15,
        "bar_filled": "‾",
        "bar_empty": "_",
        "sprintf": "mem [<span fgcolor='#FD00E1'>%s\\%s</span>]",
        "markup": "pango"
    },{
        "name": "cpu",
        "cron": "@every 5s",
        "instance": "cpu-uzg",
        "color": "#00FFD5",
        "min_width": 450,
        "align": "right",

        "bar_width": 15,
        "bar_filled": "‾",
        "bar_empty": "_",
        "sprintf": "cpu [<span fgcolor='#FD00E1'>%s\\%s</span>]",
        "markup": "pango"
    },{
        "name": "readfile",
        "cron": "@every 5s",
        "file_path": "/sys/class/thermal/thermal_zone2/temp",
        "sprintf": "<span fgcolor='#FD00E1'>%.2s℃</span>",
        "instance": "cputemp",
        "color": "#00FFD5",
        "min_width": 110,
        "align": "left",
        "markup": "pango"
    },{
        "name": "readfile",
        "cron": "@every 5s",
        "file_path": "/sys/class/drm/card0/device/hwmon/hwmon2/temp1_input",
        "sprintf": "[gpu <span fgcolor='#FD00E1'>%.2s℃</span>]",
        "instance": "gputemp",
        "color": "#00FFD5",
        "min_width": 200,
        "align": "center",
        "markup": "pango"
    },{
        "name": "datetime",
        "datetime_format": "[<span fgcolor='#FD00E1'>15:04 2/Jan</span>]",
        "cron": "0 * * * * *",
        "instance": "1",
        "color": "#00FFD5",
        "min_width": 360,
        "align": "right",
        "markup": "pango"
    }]
}
