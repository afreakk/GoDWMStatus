{
    "wmclient": "sway",
    "minimum_render_interval": "150ms",
    "modules": [{
        "name": "memory",
        "cron": "@every 5s",
        "instance": "lol",
        "color": "#00FFD5",
        "min_width": 300,
        "align": "left",

        "bar_width": 15,
        "bar_filled": "▰",
        "bar_empty": "▱",
        "sprintf": "[mem <span fgcolor='#FD00E1'>%s%s</span>]",
        "markup": "pango"
    },{
        "name": "cpu",
        "cron": "@every 5s",
        "instance": "cpu-uzg",
        "color": "#00FFD5",
        "min_width": 350,
        "align": "right",

        "bar_width": 15,
        "bar_filled": "▰",
        "bar_empty": "▱",
        "sprintf": "[cpu <span fgcolor='#FD00E1'>%s%s</span>",
        "markup": "pango"
    },{
        "name": "readfile",
        "cron": "@every 5s",
        "file_path": "/sys/class/thermal/thermal_zone2/temp",
        "sprintf": "<span fgcolor='#FD00E1'>%.2s℃</span>]",
        "instance": "cputemp",
        "color": "#00FFD5",
        "min_width": 160,
        "align": "left",
        "markup": "pango"
    },{
        "name": "readfile",
        "cron": "@every 5s",
        "file_path": "/sys/class/drm/card0/device/hwmon/hwmon2/temp1_input",
        "sprintf": "[gpu <span fgcolor='#FD00E1'>%.2s℃</span>",
        "instance": "gputemp",
        "color": "#00FFD5",
        "min_width": 160,
        "align": "center",
        "markup": "pango"
    },{
        "name": "readfile",
        "cron": "@every 5s",
        "file_path": "/sys/class/drm/card0/device/hwmon/hwmon2/pwm1",
        "sprintf": "fan_pwm <span fgcolor='#FD00E1'>%s</span>",
        "instance": "gpufanspeed",
        "color": "#00FFD5",
        "min_width": 160,
        "align": "center",
        "markup": "pango"
    },{
        "name": "readfile",
        "cron": "@every 5s",
        "file_path": "/sys/class/drm/card0/device/hwmon/hwmon2/pwm1_enable",
        "sprintf": "fan_mode <span fgcolor='#FD00E1'>%s</span>]",
        "instance": "gpufanmode",
        "color": "#00FFD5",
        "min_width": 160,
        "align": "left",
        "markup": "pango"
    },{
        "name": "datetime",
        "datetime_format": "[<span fgcolor='#FD00E1'>15:04 2/Jan</span>]",
        "cron": "0 * * * * *",
        "instance": "1",
        "color": "#00FFD5",
        "min_width": 250,
        "align": "right",
        "markup": "pango"
    }]
}
